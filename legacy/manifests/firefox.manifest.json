{
  "manifest_version": 2,
  "name": "FocusClimb",
  "short_name": "FocusClimb",
  "version": "0.3.0",
  "description": "Replace new tab page with a personal dashboard featuring , desired pictures and inspiration.",
  "icons": {
    "48": "focusClimb-48.png",
    "96": "focusClimb-96.png"
  },
  "chrome_url_overrides": {
    "newtab": "dashboard.html"
  },
  "browser_action": {
    "default_title": "FocusClimb"
  },
  "chrome_settings_overrides": {
    "homepage": "dashboard.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
} 




