{
   "background": {
      "persistent": false,
      "scripts": [ "focusclimb.js" ]
   },
   "browser_action": {
      "default_title": "Focus Climb"
   },
   "chrome_url_overrides": {
      "newtab": "dashboard.html"
   },
   "manifest_version" : 2,
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "Replace new tab page with a personal dashboard featuring , desired pictures and inspiration.",
   "name": "Focus Climb",
   "offline_enabled": true,
   "short_name": "FocusClimb",
   "version": "0.1.1"
}
